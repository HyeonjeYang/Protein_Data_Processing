   "source": [
    "from Bio import PDB\n",
    "\n",
    "parser = PDB.MMCIFParser()\n",
    "structure = parser.get_structure(\"protein\", \".cif\") #write the name of it\n",
    "\n",
    "io = PDB.PDBIO()\n",
    "io.set_structure(structure)\n",
    "io.save(\".pdb\") #write the name of it"
   ]
  }
